@charset "UTF-8";
/* CSS Document */

/* Campus Staff Site InfoBase */


#wrapper {
  width: 966px;
  margin: 0px auto;
  position: relative; 
}


/* Logo bar styles */

a.logo {
  background: url(<%= asset_path 'logo_infobase_without.jpeg' %>) no-repeat;
  width: 137px;
  height: 49px;
  display: block;
  margin: 5px 0px;
  float: left;
}

.help {
  font-size: 12px;
  margin-right: 10px;
  position: relative;
  top: 4px;
}

#helpImg {
  position: relative;
  left: 17px; 
  top: 0px;
  vertical-align:bottom;
}

#helpside {

}

#helpside a {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 10px;
}


/* Menu bar styles */

#menuBar {
  background: url(<%= asset_path 'menubg_body.png' %>);
  clear: both;
  height: 51px;
}

#menuBar ul li.selected {
  background: url(<%= asset_path 'hover_over_home_enterstats_andviewreports.gif' %>) no-repeat center;
}

#menuBar .submenubar ul li.report {
  margin-right: 138px;
}

#menuBar .submenubar ul li.ministry {
  margin-right: 54px;
}

#menuBar .submenubar ul li a.active {
  background: url(<%= asset_path 'subnav_hover.gif' %>) 50% 50% repeat-x;
}

#menuBar .submenubar ul li a:hover {
  background: url(<%= asset_path 'subnav_hover.gif' %>) 50% 50% repeat-x;
}

#menu_right {
  clear: right; 
}

#mainNav {
  width: 500px;
  height: 29px;
}

#mainNav ul {
  height: 29px;
  padding-left: 20px;
}

#mainNav ul li {
  display: inline;
  padding: 5px 15px 5px 15px;
  line-height: 29px;
  height: 29px;
}

#mainNav ul li a {
  color: #ffffff;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 15px;
  padding: 5px 0px 4px 0px;
}

#mainNav ul li:hover {
  background: url(<%= asset_path 'hover_over_home_enterstats_andviewreports.gif' %>) no-repeat center;
}

#quickSearch {
  width: 450px;
  position: relative;
  left: 500px;
  top: -26px;
}

#quickSearch select, #quickSearch input {
  margin-right: 4px;
}

#quickSearch a {
  color: #ffffff;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 9px;
}

.submenubar {
  position: absolute; 
}

.submenubar ul {
  margin-top: 2px;
}

.submenubar ul li a {
  color: #ffffff;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
  padding: 10px;
}

.submenubar ul li {
  display: inline;
  list-style-type: none;
}

#flash_notice, #flash_error {
  color: red;
  font-size: 14px;
}

/* Main content column styles */

#enterStats {
  width: 205px;
  float: left;
  margin-right: 130px;
  margin-left: 30px;
}

#steps {
  float: left;
  font-size: 35px;
  font-family: 'Trebuchet MS', Arial, sans-serif;
  font-weight: bold;
  line-height: 230px;
  margin-top: -136px;
  color: #0060AE;
}

#instruct {
  margin-left: 50px;
  width: 205px; 
}

#instruct p, #viewReports p, .formText {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
  line-height: 17px;
}

#search {
  width: 285px;
  float: left;
  margin-right: 53px;
}

#strategy {
  float: left;
  width: 136px;
}

#region {
  width: 295px; 
}

#searchInfo {
  float: left;
  clear: both;
  margin-top: 12px;
}

#viewReports {
  width: 220px;
  float: left;
}


/* Redesign of Stats Styles */
div.schoolentry {
  table.outer_stats_table {
    border: 0px solid black;
    margin: 25px;
    td.weekly_wrapper { width: 600px; }
    td.ongoing_wrapper { width: 280px; }
    table.weekly td { width: 275px; }
    .nopad { padding: 0px; }
    .nomargin { margin: 0px; }
  }
  table.ongoing {
    td h4 { margin: 3px 5px 3px 5px; }
    label.semester { padding-bottom: 0px; }
  }
  .infobox { margin: 25px; height: 5em; min-height: 5em; }
}
.txt_right { text-align: right; }

/* Footer styles */

#footer {
  clear: both;
  background: url(<%= asset_path 'footer.gif' %>) repeat-x;
  position: relative;
  top: 40px;
  padding: 20px 0 20px 30px;
  text-align:center;
  font-size: 14px;
}

#footer .copyright {
  font-style: italic;
}

#footer a {
  text-decoration: none;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
}

#footer strong {
  color: #0060AE; 
}


/* Header text styles */

h1 {
  color: #0060AE;
  font-family: Corbel, Arial, sans-serif;
  margin-bottom: 5px; 
}

h3 {
  color: #5AB4F2;
  font-family: Palatino, Times, serif;
  font-weight: lighter; 
}

h4.thin {
  margin: 0px;
}

h4.semester {
  text-align: right;
}

h4.semester_title {
  text-align: right;
  margin: 5px 0px 10px 0px;
}


/* Class styles */

.footer-text {
  position: relative;
  top: -2px;
}

.body-header {
  margin-bottom: 8px; 
}

.float-left {
  float: left;  
}

.float-right {
  float: right; 
}

.no-border {
  border: none; 
}

.select-divs {
  width: 60px;
  float: left;
}

.input-margin {
  margin-bottom: 5px; 
}

a {
  text-decoration: none;
}

img {
  border: 0px;
}

/* Styles for inner pages */

#content {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
}

.field  {
  margin-bottom: 25px;
}

.thecolumn {
  width: 420px;
}

.stats_column, .stats_column_left {
  float: left;
  width: 270px;
}

.stats_column_left {
  padding-left: 30px;
}

.stats_column_right {
  float: right;
  padding-right: 30px;
  width: 280px;
}

.semester_column, semester_column_labels {
  float: right;
  margin-left: 10px;
}

.semester_column_labels {
  padding-top: 35px;
}

#breadcrumb a {
  margin-right: 10px;
}

#breadcrumb {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
  margin-top: 8px;
  margin-right: 3px;
}

.quicksearch_open {
  position: absolute; 
}

.quicksearch_open table {
  font-family: Helvetica, Arial, sans-serif;
  font-size: 12px;
}

/* Enter Movement Indicators page */

table.movementheader {
  border-collapse: collapse;
  height: 35px;
  margin: 0;
  padding: 0;
  width: 100%;
  background-color: #8089ca;
}

.checkmark, .checkmark first_week {
  display: inline;
  list-style-type: none;
  margin-right: 10px;
}

#tab_wrapper {
  float: right;
  height: 55px;
  list-style-type: none;
}

.weekTab, .thisWeek {
  display: inline;
  float: left;
  margin-right: 0px;
  text-align: center;
}

.weekTab a {
  display: block;
  padding: 4px 6px;
  color: blue;
  width: 25px;
}

.weekTab a:hover {
  background: #efefef;
}

li.selected {
  list-style-type: none;
}

div.aschool, div.aschool_thin h2 {
  font-size: 14px;
}

a.edittext {
  font-size: 10px;
}

.aschool .thecolumn, .aschool_thin .thecolumn {
  float: left;
  margin-right: 20px;
}

.schoolentry .field label {
  font-size: 12px;
  font-family: Helvetica, Arial, sans-serif;
  font-style: normal;
  color: #000000;
  margin: 0px 12px 0px 12px;
  text-align: left;
}

label.semester {
  float: right;
  padding-bottom: 10px;
}

p.semester {
  text-align: right;
}

.stats_info_surround {
  width: 900px;
}

.schoolentry .field {
  margin-bottom: 5px;
}

.schoolentry .field input.totalnumber {
  border: 1px solid silver;
  padding: 2px;
  width: 50px;
}

.schoolentry .field input.totalnumber {
  border: 1px solid silver;
  padding: 2px;
  width: 50px;
}

.schoolentry {
  background-color: #efefef;
}

div.aschool {
  border-top: 1px solid silver;
  height: 390px;
  padding: 10px;
}

div.aschool_thin {
  border-top: 1px solid silver;
  height: 100px;
  padding: 10px;
}

div.aschool .thecolumn, div.aschool_thin .thecolumn {
  width: 31%;
}

th.bookmark {
  width: 30px;
  padding-left: 0px !important;
}

table.movementheader th {
  padding-left: 10px;
}

#enter_stat .results {
  padding-left: 0;
}

ul.results li.selected {
  height: auto;
}

ul.results li {
  display: inline-block;
  height: 35px;
  margin-bottom: 3px;
  overflow: hidden; 
}

form#fast_ll_stat ul.checkmarks li.checkmark {
  height: auto;
  margin-bottom: 0px;
  width: 26px;
  padding: 0px 4px;
  margin-right: 0px;
}

ul.checkmarks {
  padding: 0px;
}

table.movementheader th.schoolname {
  padding: 0px;
}

form#fast_ll_stat ul.results li {
  width: 966px;
}

a.editfield {
  background: url(<%= asset_path 'icons/pencil_tiny.png' %>) no-repeat scroll 0 50% transparent;
  display: inline-block;
  height: 10px;
  margin: 0 0px;
  width: 10px;
}

a.deletefield {
  background: url(<%= asset_path 'icons/delete_tiny.png' %>) no-repeat 0% 50%; 
  width: 10px; 
  display: inline-block; 
  height:10px; 
  width: 10px; 
  margin: 0 5px;
}

th.expand, td.expand {
  background: url(<%= asset_path 'expand.png' %>) no-repeat scroll 50% 50% transparent;
  width: 60px;
}

ul.results th.expand {
  background: url(<%= asset_path 'contract.png' %>) no-repeat scroll 50% 50% transparent;
  width: 35px;
  padding: 0px;
}

li.checkmark img, ul.results li.checkmark img {
  /* background-color: #d3d3d3; */
  padding: 1px;
}

li.first_week img, ul.results li.first_week img {
  /* background-color: #d3d3d3; */
  border: 1px solid #6770b1;
  background: #6770b1;
}

a.unbookmark, .editicon {
  background: url(<%= asset_path 'icons/bookmark.png' %>) no-repeat scroll 0 50% transparent;
  font-weight: normal;
  padding-left: 20px;
}

.editicon {
  background: url(/assets/icons/pencil_tiny.png) no-repeat scroll 0 50% transparent;
}

.stats_submit_surround {
  clear: both;
}

/* Ministry Locations page */

h3.indented {
  font-style: normal;
  text-indent: 0.5em;
}

.strategy h3 {
  background: none repeat scroll 0 0 #3756ab;
  border-bottom: 1px solid #3756ab;
  margin: 0;
  padding: 6px;
  font-family: Helvetica, Arial, sans-serif;
  font-size: 14px;
  font-style: normal;
  font-weight: bold;
  color: #ffffff;
}

.strategy {
  border: 1px solid #3756ab;
  margin-bottom: 1em;
  background-color: #a2adff;
  padding: 6px;
}

.strategy .details .field {
  display: inline;
  line-height: 1.5em;
}

.strategy .details .thecolumn {
  float: left;
  width: 50%;
}

.strategy .details .box {
  display: block;
  margin-left: 100px;
  margin-top: -18px;
}

.details .thecolumn .field label {
  color: #000000;
  font-size: 12px;
  font-style: normal;
  font-family: Helvetica, Arial, sans-serif;
  text-align: left;
  width: 100px;
  line-height: 20px;
  padding-right: 8px;
}

.strategy .details {
}

.clear {
  clear: both;
}

div.aschool:hover, div.aschool_thin:hover {
  /* background-color:#e0e0e0; */
}

h1.multi {
  float: left;
}

h1 span.title {
  display: block;
  font-size: 16px;
}

a.unbookmark {
  padding-left: 23px;
}

.editbtn {
  border-radius: 5px;
  padding: 4px 4px 4px 22px;
  background-color: white !important;
  background-position: 4px 50% !important;
  box-shadow: 0 0 5px rgba(0,0,0,.1);
  border: 1px solid silver;
  vertical-align: middle;
}

.field {
  line-height: 1.5em;
}

p {
  font-size: 12px;
}

.aschool h2 {
  margin-bottom: 5px;
}

div.keyitem {
  margin-top: 10px;
  padding: 8px 8px 8px 35px;
  font-size: 12px;
  font-weight: bold;
  background: #FAF3D4 url(/assets/bulb.png) no-repeat 7px 50%;
}
.keyitem_bg {
  background: #FAF3D4;
}

span.positive {
	color: #007f00;
}

span.negative {
	color: #bf0000;
}

table#styled_form_table td#label_column {
	width: 200px;
}

table#styled_form_table td#display_label_column {
	width: 120px;
}

table#styled_form_table td#field_column {
	width: 500px;
}

table#styled_form_table td {
	vertical-align: top;
}

div.merge.person {
  margin: 10px;
  padding: 10px;
  border: 1px solid black;
  width: 270px;
}

div.merge.person.preview {
  float:right;
}

.confirm_merge div.merge.person {
  float:left;
}
